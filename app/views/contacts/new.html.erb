<% provide(:title, "Contact Us") %>
<%
	keywords %q(contact)
%>
	<div id="susnav"><%= render 'layouts/navbar2' %></div>
<div class="container-fluid" id="contact">
<div class="row">
	<section id="contactbanner">
		
		<div class="container sections">
			<h2 class="sectiontitlepage">Contact Us</h2>
		</div>
	</section>
</div>
<div class="row contactpage">
 <div class="container">
  <div class="row contactrow">
	<div class="col-md-12 col-sm-12 col-xs-12 contactform">
	     
		<%= form_for(@contact) do |f| %>
		    <div class="row formrow">
			<div class="col-md-2 col-sm-2 col-xs-2 formcol">
			    <%= f.label :name %>
			</div>
			<div class="col-md-10 col-sm-6 col-xs-12">
			    <%= f.text_field :name %>
			</div>
		    </div>
		    <div class="row formrow">
			<div class="col-md-2 col-sm-2 col-xs-2 formcol">
			    <%= f.label :email %>
		    	</div>
			<div class="col-md-10 col-sm-6 col-xs-12">
			    <%= f.text_field :email %>
			</div>
		    </div>
		    <div class="row formrow">
			<div class="col-md-2 col-sm-2 col-xs-2 formcol">
			    <%= f.label "Message" %>
			</div>
			<div class="col-md-10 col-sm-6 col-xs-12">
			    <%= f.text_area :body, :rows => 15 %>
			    <div class="hidden"><%= f.text_area :nickname, :hint => 'Leave this field blank!' %></div>
			    <%= f.submit "Submit", :as => :button, class: "btn btn-large btn-sun" %>
			</div>
		    </div>
		<p class="notice"><%= notice %></p>
 	        <p class="alert"><%= alert %></p>
			   

		      	    

		<% end %>
	</div>
  </div>
 </div>
</div>

<div class="row footertact">
  <div class="col-md-6 col-sm-12 col-xs-12" id="footer">
	Â© 2014 George's Seafood, Inc.
  </div>
</div>
</div>
