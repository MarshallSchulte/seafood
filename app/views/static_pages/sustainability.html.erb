	{
		console.log("Mobile");
	}
	else
	{
		console.log("desktop");
		$.stellar( {hideDistantElements: true} );
		$(window).resize(function() {
		   $.stellar('refresh');
		});
	}
});
$(window).load(function(){
	$.stellar('refresh');
});
</script>
